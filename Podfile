# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'SJMPlayer' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!  :linkage => :static

  # Pods for SJMPlayer
  pod 'SJUIKit/SQLite3', :podspec => 'https://gitee.com/changsanjiang/SJUIKit/raw/master/SJUIKit-YYModel.podspec'
  pod 'SJBaseVideoPlayer/IJKPlayer'
  pod 'ijkplayerssl', :git => 'https://gitee.com/changsanjiang/ijkplayer.git'
  pod 'SJVideoPlayer'
  pod 'SJMediaCacheServer'
end
